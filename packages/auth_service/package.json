{
  "name": "@turnazos/user_service",
  "version": "1.0.0",
  "description": "This is the user service of turnazos",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node ./dist/src/server",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:rest": "prisma migrate reset",
    "prisma:watch": "nodemon -w ./prisma -e .prisma -x 'yarn run prisma:generate'",
    "server:watch": "nodemon -r tsconfig-paths/register ./src/server.ts",
    "docker:local-compose": "docker-compose -f ./docker-compose.local.yml up -d",
    "dev": "yarn docker:local-compose && concurrently 'yarn run prisma:watch' 'yarn run server:watch'",
    "build": "tsc",
    "prepare": "husky install",
    "test": "jest",
    "cm": "cz"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.9.50",
    "redis": "^4.0.6",
    "whatsapp-business": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/amqplib": "^0.8.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "commitizen": "^4.2.4",
    "concurrently": "^7.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "devmoji": "^2.3.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "http-status-codes": "^2.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.5",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "prisma": "^4.9.0",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^4.6.3"
  },
  "author": "myself",
  "license": "ISC"
}
